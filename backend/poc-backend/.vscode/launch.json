{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to SAM CLI",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5656, // set the port to whatever you would like, but it has to match the port in the --debug-port flag
      "localRoot": "${workspaceRoot}/src/webhookHandler",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
    }
  ]
}
