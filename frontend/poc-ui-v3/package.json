{
  "name": "samantha",
  "version": "0.3.0",
  "description": "Bellhop 0.3.0 React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Bellhop Inc.",
  "license": "MIT",
  "scripts": {
    "prebuild": "rebuild-node-sass",
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "prestart": "(cat certs/cert.pem certs/key.pem > ./node_modules/webpack-dev-server/ssl/server.pem) || :",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js --sorcemap --https --host 0.0.0.0 --port 2000",
    "prod": "yarn run build && node express.ts --https",
    "start-prod": "node express.ts --https",
    "chromatic": "chromatic",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "prebuild:storybook": "yarn run test:generate-output",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 2040 ",
    "docz:dev": "docz dev",
    "docz:build": "docz build --prefix-paths=.",
    "docz:serve": "docz build && docz serve"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "chromatic": "^4.0.2",
    "css-loader": "^3.4.2",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.2.7",
    "node-sass": "^4.14.1",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-is": "^16.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.48",
    "@hot-loader/react-dom": "^16.13.0",
    "@mdx-js/loader": "^1.6.1",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/theming": "^5.3.18",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-router-dom": "^5.1.4",
    "ace-builds": "^1.4.11",
    "animated": "^0.2.2",
    "aws-amplify": "^3.0.8",
    "babel-preset-react-app": "^9.1.2",
    "bootstrap": "^4.4.1",
    "docz": "latest",
    "eslint": "^6.8.0",
    "graphql": "^15.0.0",
    "jquery": "^3.5.0",
    "markdown-to-jsx": "^6.11.1",
    "node-jsx": "^0.13.3",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.7.2",
    "react-ace": "^8.1.0",
    "react-animated-css": "^1.2.1",
    "react-bootstrap": "^1.0.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.0",
