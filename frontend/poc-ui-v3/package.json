    "regenerator-runtime": "^0.13.5",
    "shallowequal": "^1.1.0",
    "source-map-loader": "^0.2.4",
    "strip-ansi": "^6.0.0",
    "ts-loader": "^7.0.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "url-loader": "^4.1.0",
