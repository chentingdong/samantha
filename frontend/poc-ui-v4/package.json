{
  "name": "samantha",
  "version": "0.3.0",
  "description": "Bellhop 0.3.0 React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Bellhop Inc.",
  "license": "MIT",
  "scripts": {
    "build:tailwind": "tailwindcss build src/assets/tailwind/tailwind.css -o src/assets/tailwind/tailwind.generated.css",
    "prebuild": "rebuild-node-sass",
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "prestart": "(cat certs/cert.pem certs/key.pem > ./node_modules/webpack-dev-server/ssl/server.pem) && yarn run prebuild && yarn run build:tailwind",
    "start": "yarn run start-dev",
    "start-dev": "NODE_ENV=development webpack-dev-server --config=configs/webpack/dev.js --sourcemap --http --port 2000",
    "prod": "yarn run build && yarn run start-prod",
    "start-prod": "node express.ts",
    "chromatic": "chromatic",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "prebuild:storybook": "yarn run build:tailwind && yarn run test:generate-output",
    "build-storybook": "build-storybook",
    "storybook": "yarn run prebuild && yarn run build:tailwind && start-storybook -p 2040 ",
    "docz:dev": "docz dev",
    "docz:build": "docz build --prefix-paths=.",
    "docz:serve": "docz build && docz serve"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
