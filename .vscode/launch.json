{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Backend",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/backend/poc",
      "program": "${workspaceRoot}/backend/poc/node_modules/.bin/sls",
      "args": [
        "offline",
        "start"
      ]
    },
    {
        "name": "UI",
        "type": "chrome",
        "request": "launch",
        "port": 2622,
        "url": "https://localhost:2000/demo",
        "webRoot": "${workspaceRoot}",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
            "webpack:///./~/*": "${workspaceRoot}/../node_modules/*",
            "webpack:///../*": "${workspaceRoot}/../node_modules/*",
            "webpack:///./*": "${workspaceRoot}/../*",
            "webpack:///*": "/*"
        }
    }
  ]
}
