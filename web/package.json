    "#prestart": "yarn run prebuild",
    "prebuild": "yarn clean && rebuild-node-sass && yarn run build:theme",
    "build": "NODE_ENV=production BABEL_ENV=production webpack -p --config=configs/webpack/prod.js ",
    "build-storybook": "build-storybook",
    "build:images": "src/assets/img/build-images.sh",
    "build:rsuite": "src/assets/rsuite/build-rsuite.sh",
    "build:tailwind": "src/assets/tailwind/build-tailwind.sh",
    "build:theme": "yarn build:tailwind && yarn build:rsuite && yarn build:images",
    "chromatic": "npx chromatic --project-token=jn5n1c1u9l",
    "clean": "rimraf dist/*",
    "lint": "eslint ./src",
    "prod": "yarn run build && yarn run start:prod",
    "start": " yarn start:dev",
    "start:dev": "NODE_ENV=development PUBLIC_URL='http://localhost:2000' webpack-dev-server --config=configs/webpack/dev.js --sourcemap --http --host 0.0.0.0 --port 2000",
    "start:prod": "NODE_ENV=production node express.ts",
    "storybook": "yarn run prebuild && start-storybook -p 2040 ",
    "storybook:build": "build-storybook",
    "storybook:deploy": "yarn chromatic",
    "storybook:prebuild": "yarn run build:theme && yarn run test:generate-output",
    "test": "jest --config configs/jest.config.js"
  },
