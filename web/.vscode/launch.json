{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Storybook Debug",
      "url": "http://localhost:2040",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*",
        "webpack:///./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///./~/*": "${webRoot}/node_modules/*"
      }
    },
    {
      "name": "UI",
      "type": "chrome",
      "request": "launch",
      "port": 2622,
      "url": "https://localhost:2000",
      "webRoot": "${workspaceRoot}/src",
      "sourceMaps": true
    }
  ]
}
