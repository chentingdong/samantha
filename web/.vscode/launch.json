{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Nightwatch, webrtc",
      "envFile": "${workspaceFolder}/tests/nightwatch/.env",
      "program": "${workspaceFolder}/node_modules/nightwatch/bin/runner.js",
      "stopOnEntry": false,
      "args": [
        "-c",
        "tests/nightwatch/nightwatch.conf.js",
        "--test",
        "tests/nightwatch/tests/webrtc.js",
        "-e",
        "chrome_1"
      ],
      "sourceMaps": false,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Storybook Debug",
      "url": "http://localhost:2040",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/*",
        "webpack:///./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///./~/*": "${webRoot}/node_modules/*"
      }
    },
    {
      "name": "UI",
      "type": "chrome",
      "request": "launch",
      "port": 2622,
      "url": "http://localhost:2000/bells/hFLJznk0hv9hiRGihEGET",
      "webRoot": "${workspaceRoot}/src",
      "sourceMaps": true
    }
  ]
}
