{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetAccessPoint",
        "cognito-identity:*",
        "s3:PutAccountPublicAccessBlock",
        "s3:GetAccountPublicAccessBlock",
        "s3:ListAllMyBuckets",
        "s3:ListAccessPoints",
        "s3:ListJobs",
        "mobileanalytics:PutEvents",
        "s3:CreateJob",
        "s3:HeadBucket",
        "cognito-sync:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject"],
      "Resource": [
        "arn:aws:s3:::samantha-files/public/*",
        "arn:aws:s3:::samantha-files/protected/*",
        "arn:aws:s3:::samantha-files/private/*"
      ]
    }
  ]
}
